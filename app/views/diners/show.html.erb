<div class="page-header">
  <h3>Diner #<%= @diner.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Party</dt>
      <dd><%= @diner.party_id %></dd>

      <dt>User</dt>
      <dd><%= @diner.user_id %></dd>

      <dt>Diner Id</dt>
      <dd><%= @diner.id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/diners" class="btn btn-primary">
        Back
      </a>
      <a href="/diners/<%= @diner.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_diner/<%= @diner.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
