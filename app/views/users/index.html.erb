<div class="page-header">
  <h3>All Users</h3>
</div>

<div class="row">
  <div class="col-md-9">
    <table class="table table-striped table-hover">
      <th> Avatar </th>
      <th> Username </th>
      <th> Email </th>

      <% @users.each do |user| %>
    <tr>  <td><img width=200 height=200 src="<%= user.image %>"></td>
  <td> <%= user.username%> <i class="fa fa-external-link"><a href="/users/<%= user.id %>"></a></i></td>
      <td> <%= user.email%> </td> </tr>
      <% end %>
</div>
</div>
